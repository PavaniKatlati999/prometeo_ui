{
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "extensions": [
      "js",
      "jsx",
      "tsx",
      "ts"
    ],
    "ignore": [
      "**/*.(test|spec).ts"
    ],
    "babelrc": false,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "recurseDepth": 10,
  "source": {
    "include": "./src",
    "includePattern": ".+\\.ts(x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./doc/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  }
}
