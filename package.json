{
  "name": "prometeo",
  "version": "0.0.1",
  "description": "Arcus Power Corp UI library",
  "copyright": "Arcus Power Corp",
  "homepage": "/",
  "target": "web",
  "author": {
    "name": "Arcus Power Corp",
    "url": "https://arcuspower.com/"
  },
  "main": "./init.tsx",
  "jsnext:main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "private": true,
  "deprecated": false,
  "bugs": {
    "url": "https://bitbucket.org/tymeware/prometeo/issues"
  },
  "bin": {
    "library": "./dist/index.js"
  },
  "files": [
    "./dist/"
  ],
  "app": {
    "folder": "/",
    "code": "prometeo",
    "name": "Arcus UI library",
    "title": "Arcus Power Corp UI library",
    "type": "beta"
  },
  "scripts": {
    "make-dist": "webpack --mode=production --config=webpack.config.js",
    "git": "git add . && git commit -m \"update\" && git pull && git push --all",
    "backup": "cd mcp/zip && run.bat",
    "watch": "npm run make-dist && webpack --watch --mode development --config webpack.config.js ",
    "tsc": "tsc -b",
    "eslint": "eslint",
    "merge2Master": "cd mcp/javascripts && node gitMergeMaster",
    "update": "cd mcp/javascripts && node npmUpdate.js",
    "make-tgz": "npm run make-dist && npm pack",
    "jsdoc": "jsdoc -r -c ./jsDocConfig.json",
    "build": "webpack --config webpack.config.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/tymeware/prometeo.git"
  },
  "paths": {
    "*": [
      "./dist/types/*"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 Chrome major versions",
      "last 2 Firefox major versions",
      "last 2 Safari major versions",
      "last 2 Edge major versions",
      "last 2 Opera versions",
      "last 2 iOS major versions",
      "last 1 Explorer major version",
      "last 1 ChromeAndroid version",
      "last 1 UCAndroid version",
      "last 1 Samsung version",
      "last 1 OperaMini version",
      "Firefox ESR",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.25.6",
    "@babel/core": "7.25.2",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "7.25.4",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@babel/runtime": "^7.25.6",
    "@storybook/addon-docs": "9.1.4",
    "@storybook/addon-onboarding": "9.1.4",
    "@storybook/addon-webpack5-compiler-swc": "4.0.1",
    "@storybook/react-webpack5": "9.1.4",
    "@tanstack/react-table": "^8.7.3",
    "@types/browserify": "latest",
    "@types/jest": "29.5.12",
    "@types/node": "22.5.3",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "babel-loader": "9.1.3",
    "browserify": "latest",
    "classnames": "^2.5.1",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "6.10.0",
    "dotenv": "16.4.5",
    "eslint": "9.9.1",
    "eslint-plugin-storybook": "9.1.4",
    "global-jsdom": "^24.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jquery": "3.7.1",
    "jsdoc": "^4.0.3",
    "jsdoc-babel": "0.5.0",
    "jsdom": "25.0.0",
    "less": "4.2.0",
    "less-loader": "12.2.0",
    "progress-bar-webpack-plugin": "2.1.0",
    "progress-webpack-plugin": "1.0.16",
    "prop-types": "15.8.1",
    "rollup": "^3.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.0",
    "sass": "1.92.0",
    "sass-loader": "16.0.1",
    "storybook": "9.1.4",
    "style-loader": "4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "9.5.2",
    "typescript": "^5.0.0",
    "webpack": "5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@mantine/core": "8.2.5",
    "@mantine/hooks": "8.2.5",
    "@storybook/test": "8.6.14",
    "@tanstack/react-query": "5.85.5",
    "@tanstack/react-table": "8.21.3",
    "@tanstack/react-virtual": "3.13.12",
    "axios": "1.11.0",
    "dayjs": "1.11.13"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
