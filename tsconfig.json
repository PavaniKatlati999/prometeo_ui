{
  "compilerOptions": {
    "lib": [
      "es2022",
      "es2021",
      "es2020",
      "es2016.array.include",
      "es2017.object",
      "es5",
      "es6",
      "dom",
      "dom.iterable"
    ],
    "noEmit": true,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "importHelpers": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "baseUrl": ".",
    "outDir": "./dist/",
    "module": "commonjs",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es2022",
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "paths": {}
  },
  "types": [
    "node",
  ],
  "includes": [
    "./types/*"
  ],
  "exclude": [
    "node_modules",
    ".old",
    ".OLD",
  ]
}
